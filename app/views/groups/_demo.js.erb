function getMetaBrightUser(email, token){
  var endpoint = "/groups/users";
  $.ajax({
    url: endpoint, 
    data: { email: email, token: token},
    success: function(data, status, xhr){
      console.log(data);
    },
    dataType: "json"
  });
}

var email = "<%= user.email %>";
var token = "<%= group.token %>";

getMetaBrightUser(email, token);