:css  ul.inlinedisplay { height: inherit; }  div.mosaic-block.bar2 { height: 130px; }%div.single_pane  = link_to "Create New Challenge", new_path_path, class: "button button-actionable", style: "float:right; margin-top: 5px"  %h1 My Creations  %hr  %div{ style: "margin-bottom: 40px;" }    - if @paths.empty?      %p You have not created any Challenges yet. You can get started by #{link_to "creating one here", new_path_path}    - else    %ul      %ul.inlinedisplay        - @paths.each do |path|          = render "shared/mosaic", { pic: path.path_pic, link: edit_path_path(path.permalink), title: path.name, subtitle: "#{path.enrollments.size} users" }