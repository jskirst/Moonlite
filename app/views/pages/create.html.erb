<section>  <div class="input" style="float:right; margin-top: 5px">    <%= link_to "Build New "+name_for_paths, new_path_path, :class => "btn primary" %>  </div>  <div><a><h1>My <%= name_for_paths.pluralize %></h1></a></div></section><hr/><section><div style="margin-bottom: 40px;">  <h3>Published <%= name_for_paths.pluralize %></h3>  <% if @published_paths.empty? %>  <p>You have not created or published any <%= name_for_paths.pluralize %> yet. You can get started by <%= link_to "creating one here", new_path_path %>.</p>  <% else %>  <ul class="media-grid">  <% @published_paths.each do |path| %>    <li>      <a href="<%= edit_path_path(path) %>" id="<%= path.name.gsub(" ", "_") %>">        <div class="path-caption">          <span class="caption-title"><%= truncate(path.name, :length => 35) %></span><br>          <span style="font-size: 10px; float: left;"><%= path.enrolled_user_count %> users enrolled.</span>          <span style="font-size: 10px; float: right;"><%= path.difficulty_rating %></span>        </div>        <img class="thumbnail" src="<%= path.path_pic %>" height="150" width="210">      </a>    </li>  <% end %>  </ul><% end %></div><% unless @unpublished_paths.empty? %>  <div style="margin-bottom: 40px;">    <h3>Unpublished <%= name_for_paths.pluralize %></h3>    <ul class="media-grid">    <% @unpublished_paths.each do |path| %>      <li>        <a href="<%= edit_path_path(path) %>" id="<%= path.name.gsub(" ", "_") %>">          <div class="path-caption">            <span class="caption-title"><%= truncate(path.name, :length => 35) %></span><br>            <span style="font-size: 10px; float: left;"><%= path.enrolled_user_count %> users enrolled.</span>            <span style="font-size: 10px; float: right;"><%= path.difficulty_rating %></span>          </div>          <img class="thumbnail" src="<%= path.path_pic %>" height="150" width="210">        </a>      </li>    <% end %>    </ul>  </div><% end %><% unless @collaborating_paths.empty? %>  <h3>Collaborating <%= name_for_paths.pluralize %></h3>  <ul class="media-grid">  <% @collaborating_paths.each do |path| %>    <li>      <a href="<%= edit_path_path(path) %>" id="<%= path.name.gsub(" ", "_") %>">        <div class="path-caption">          <span class="caption-title"><%= truncate(path.name, :length => 35) %></span><br>          <span style="font-size: 10px; float: left;"><%= path.enrolled_user_count %> users enrolled.</span>          <span style="font-size: 10px; float: right;"><%= path.difficulty_rating %></span>        </div>        <img class="thumbnail" src="<%= path.path_pic %>" height="150" width="210">      </a>    </li>  <% end %>  </ul><% end %></section>