<section>	<div class="input" style="float:right; margin-top: 5px">		<%= link_to "Build New Challenge", new_path_path, :class => "btn primary" %>	</div>	<div><a><h1>My Challenges</h1></a></div></section><h3>Published Paths</h3><ul class="media-grid"><% @published_paths.each do |path| %>	<li>		<a href="<%= path_path(path) %>" id="<%= path.name.gsub(" ", "_") %>">			<div class="path-caption">				<span class="caption-title"><%= truncate(path.name, :length => 35) %></span><br>				<span style="font-size: 10px; float: left;"><%= path.enrolled_user_count %> users enrolled.</span>				<span style="font-size: 10px; float: right;"><%= path.difficulty_rating %></span>			</div>			<img class="thumbnail" src="<%= path.path_pic %>" height="150" width="210">		</a>	</li><% end %></ul><h3>Unpublished Paths</h3><ul class="media-grid"><% @unpublished_paths.each do |path| %>	<li>		<a href="<%= path_path(path) %>" id="<%= path.name.gsub(" ", "_") %>">			<div class="path-caption">				<span class="caption-title"><%= truncate(path.name, :length => 35) %></span><br>				<span style="font-size: 10px; float: left;"><%= path.enrolled_user_count %> users enrolled.</span>				<span style="font-size: 10px; float: right;"><%= path.difficulty_rating %></span>			</div>			<img class="thumbnail" src="<%= path.path_pic %>" height="150" width="210">		</a>	</li><% end %></ul>