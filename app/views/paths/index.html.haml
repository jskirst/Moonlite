:css  body{ background-image: url("https://s3.amazonaws.com/moonlite-nsdub/static/Backgrounds/gplaypattern.png"); background-repeat: repeat; background-size: auto; }  .aboutcontainer{ width: 943px; margin-top: 35px;}#employer.aboutcontainer  .aboutbox    = link_to "Create New Challenge", (@group ? new_group_path_url(@group) : new_path_path), class: "button button-actionable", style: "float:right; margin-top: 5px"    %h1 My Creations    %hr    %div{ style: "margin-bottom: 40px;" }      - if @paths.empty?        %p You have not created any Challenges yet. You can get started by #{link_to "creating one here", (@group ? new_group_path_url(@group) : new_path_path)}      - else      %ul        %ul.inlinedisplay{ style: "height: inherit;" }          - @paths.each do |path|            = render "shared/mosaic", { pic: path.path_pic, link: edit_path_path(path.permalink), title: path.name, subtitle: "#{path.enrollments.size} users" }