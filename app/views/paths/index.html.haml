:css  body{ background-image: url("https://s3.amazonaws.com/moonlite-nsdub/static/Backgrounds/gplaypattern.png"); background-repeat: repeat; background-size: auto; }  .container.body { width: inherit; }  .aboutcontainer{ width: 960px; margin: -25px auto 0 auto; border-radius: 0; }  .aboutbox .header { margin-bottom: 10px;}  .aboutbox .header img { height: 72px; margin-top: -15px; }  .aboutbox .header h2 { display: inline-block; font-size: 30px; }  .aboutbox .header hr { margin: 8px; }  .body { padding: 20px 0px; }  #api_result h1 { font-size: 22px; }  #api_result img { width: 40px; height: 40px; margin: 10px; }  h3 { font-size: 22px; margin-bottom: 10px; }  .new_path_placeholder img { opacity: 0.7; margin-top: -35px; }#employer.aboutcontainer  .aboutbox    .header      %img{ src:"https://s3.amazonaws.com/moonlite-nsdub/static/stoney+100x150.png", alt: "Stoney"}      %h2 My Challenges          .body{ style: "margin-bottom: 40px;" }      .custom_challenges        %ul.inlinedisplay          - @paths.each do |path|            = render "shared/mosaic", { pic: path.path_pic, link: edit_path_path(path.permalink), title: path.name, subtitle: "#{path.enrollments.size} users" }          = render "shared/mosaic", { pic: ICON_NEW_CHALLENGE, link: (@group ? new_group_path_url(@group) : new_path_path), title: "Create your own Challenge", subtitle: "", custom_class: "new_path_placeholder" }