:css
  body{ background-image: url("https://s3.amazonaws.com/moonlite-nsdub/static/Backgrounds/gplaypattern.png"); background-repeat: repeat; background-size: auto; }
  .aboutcontainer{ width: 950px; background-color:rgba(36, 72, 109, 0.8);}


#evaluation_manager.aboutcontainer
  %div.aboutbox{ style: "background-color: white; border-radius: 4px; padding:8px;" }
    = image_tag STONEY_SMALL_URL, style:"display:inline; height:115px;", alt: "Stoney"
    %span{ style: "font-size: 50px;position: relative;top:13px;" } Evaluation Manager
    
    = link_to "Edit", edit_group_evaluations_path(@group, @evaluation), class: "button button-small", style: "float: right; margin: 80px 10px 0 0;"
    = link_to "Back", group_evaluations_path(@group), class: "button button-info button-small", style: "float: right; margin: 80px 10px 0 0;"
    
    %hr{style:"margin:0 0 20px 0;"}
    .body{ style: "padding: 0 12px;"}
      %table.table
        %thead
          %tr
            %th Name
            %th Email
            - @paths.each do |p|
              %th= p.name
            %th Date Submitted
        %tbody
          - @evaluations.each do |e|
            %tr
              %td= e.name
              %td= e.email
              - @paths.each do |p|
                %td= e["e#{p.id}_metapercentile"]
              %td= time_ago_in_words(e.submitted_at)
          - if @evaluations.empty?
            %tr
              %td{ colspan: 4 } No one has taken this evaluation yet.